<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="WeavidBundle\Entity\CourseLecture" table="course_lecture">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="createdAt" type="datetime" column="created_at" nullable="true"/>
        <field name="updatedAt" type="datetime" column="updated_at" nullable="true"/>
        <many-to-one target-entity="Course" field="course" fetch="LAZY" inversed-by="courseLectureAssociation">
            <join-columns>
                <join-column name="course_id" referenced-column-name="id" />
            </join-columns>
        </many-to-one>
        <many-to-one target-entity="Lecture" field="lecture" fetch="LAZY" inversed-by="courseLectureAssociation">
            <join-columns>
                <join-column name="lecture_id" referenced-column-name="id" />
            </join-columns>
        </many-to-one>
        <one-to-one field="previousLecture" target-entity="CourseLecture" inversed-by="nextLecture">
            <join-column name="previous_lecture_id" referenced-column-name="id" />
        </one-to-one>
        <one-to-one field="nextLecture" target-entity="CourseLecture" mapped-by="previousLecture" />
    </entity>
</doctrine-mapping>
